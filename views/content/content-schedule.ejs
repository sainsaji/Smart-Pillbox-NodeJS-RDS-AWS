<div class="boxContent">
    <form>
        <div class="form-group">
          <label for="schedulername">Scheduler Name</label>
          <input type="text" class="form-control" id="schedulername" aria-describedby="emailHelp" placeholder="Enter email">
          <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Pill Name</label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
        </div>
        <div class="form-group">
            <label for="dateRange">Date Time Picker</label>
            <input type="text" name="datetimes" />
          </div>
        
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    
    <script>
    $(function() {
    $('input[name="datetimes"]').daterangepicker({
        timePicker: true,
        startDate: moment().startOf('hour'),
        endDate: moment().startOf('hour').add(32, 'hour'),
        locale: {
        format: 'M/DD hh:mm A'
        }
    });
    });
    </script>
</div>